#!/bin/bash
uuuif [ -f ~/.gituserpass$1 ];then
  . ~/.gituserpass$1
else
  echo Github User and Password?
  read U P
  echo "U=$U \nP=$P">~/.gituserpass$1
fi

tee<<pyfile>/tmp/typ.py
#!/usr/bin/python3
import os,sys,	time
from pynput.keyboard import Key,Controller
jp=Controller()
time.sleep(3)
jp.type('$U')
jp.press(Key.enter)
jp.release(Key.enter)
time.sleep(2)
jp.type('$P')
jp.press(Key.enter)
jp.release(Key.enter)
pyfile

python3 /tmp/typ.py &

git status
git commit -m 1
git push -u origin master
